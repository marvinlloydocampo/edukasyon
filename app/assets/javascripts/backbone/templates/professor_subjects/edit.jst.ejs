<h1>Edit professor_subject</h1>

<form id="edit-professor_subject" name="professor_subject">
  <div class="field">
    <label for="subject_id"> Class Name:</label>
    <select id="subject_id" name="subject_id" value="<%= model.subject_id() %>">
      <% subjects.each(function(subject) { %>
        <% if (model.subject_id() == subject.id) { %>
          <option value="<%= subject.id %>" selected='selected'><%= subject.name() %></option>
        <% } else { %>
          <option value="<%= subject.id %>"><%= subject.name() %></option>
         <% } %>
      <% }); %>
    </select>
  </div>

  <div class="field">
    <label for="professor_id"> Professor Name:</label>
    <select id="professor_id" name="professor_id" value="<%= model.professor_id() %>">
      <% professors.each(function(professor) { %>
        <% if (model.professor_id() == professor.id) { %>
          <option value="<%= professor.id %>" selected='selected'><%= professor.full_name() %></option>
        <% } else { %>
          <option value="<%= professor.id %>"><%= professor.full_name() %></option>
         <% } %>
      <% }); %>
    </select>
  </div>

  <div class="actions">
    <input type="submit" value="Update Professor subject" />
  </div>

</form>

<a href="#/index">Back</a>
